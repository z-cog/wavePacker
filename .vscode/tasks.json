                "-fdiagnostics-color=always",
                "-I${workspaceFolder}/include",
                "-L${workspaceFolder}/lib",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/lib/libmidifile.a",
                "-o",
                "${workspaceFolder}/build/wavePacker.exe"
